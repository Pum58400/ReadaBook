<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/libraryFragment">
    <fragment
        android:id="@+id/libraryFragment"
        android:name="buu.example.myapp.LibraryFragment"
        android:label="LibraryFragment" >
        <action
            android:id="@+id/action_libraryFragment_to_detailFragment"
            app:destination="@id/detailFragment"
            app:popUpTo="@+id/libraryFragment" />
        <action
            android:id="@+id/action_libraryFragment_to_addbookFragment"
            app:destination="@id/addbookFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="buu.example.myapp.detailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" >
        <action
            android:id="@+id/action_detailFragment_to_BOOK1_1Fragment"
            app:destination="@id/BOOK1_1Fragment"
            app:popUpTo="@+id/libraryFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="buu.example.myapp.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/BOOK1_1Fragment"
        android:name="buu.example.myapp.BOOK1_1Fragment"
        android:label="BOOK1_1Fragment"
        tools:layout="@layout/fragment_book1" />
    <fragment
        android:id="@+id/addbookFragment"
        android:name="buu.example.myapp.addbook.AddbookFragment"
        android:label="fragment_addbook"
        tools:layout="@layout/fragment_addbook">
        <action
            android:id="@+id/action_addbookFragment_to_showbookFragment"
            app:destination="@id/showbookFragment" />
    </fragment>
    <fragment
        android:id="@+id/showbookFragment"
        android:name="buu.example.myapp.showbook.ShowbookFragment"
        android:label="fragment_showbook"
        tools:layout="@layout/fragment_showbook" >
        <action
            android:id="@+id/action_showbookFragment_to_addbookFragment"
            app:destination="@id/addbookFragment" />
    </fragment>
</navigation>